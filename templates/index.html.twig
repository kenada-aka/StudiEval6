<!doctype html>
<html lang="fr">
	<head>
		<meta charset="utf-8">
		<title>Secret Agency X</title>
		<!-- Balises Meta générales -->
		<meta name="Content-Type" content="UTF-8">
		<meta name="Content-Language" content="fr">
		<meta name="Description" content="Gestion de données d'une agence de surveillance.">
		<meta name="Keywords" content="agence,surveillance,gestion">
		<meta name="Subject" content="Secret Agency X">
		<meta name="Copyright" content="David">
		<meta name="Author" content="David">
		<meta name="Publisher" content="David">
		<meta name="Identifier-Url" content="http://www.secret-agency-x.com">
		<meta name="Reply-To" content="contact@secret-agency-x.com">
		<!-- Balises Meta spécifiques aux robots -->
		<meta name="Revisit-After" content="15 days">
		<meta name="Robots" content="all">
		<meta name="Rating" content="general">
		<meta name="Distribution" content="global">
		<meta name="Geography" content="France">
		<meta name="Category" content="Agency">
        <!-- Bootstrap Responsive meta tag -->
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- BoxiconsCDN -->
        <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
		<!-- BootstrapCDN -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
        <!-- Style (Media Queries) -->
		<link rel="stylesheet" href="/assets/css/style.css">
		<!-- Jquery CDN -->
		<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
		<!-- Bootstrap Javascript CDN -->
