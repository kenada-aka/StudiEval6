
{% if attribute(data, property)|length != 0 %}
    <ul class="list-group">
        {% for child in attribute(data, property) %}
            <li class="list-group-item">{{ attribute(child, childProperty) }}</li>
        {% endfor %}
    </ul>
{% endif %}
